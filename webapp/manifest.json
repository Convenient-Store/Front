{
  "sap.app": {
    "id": "capstone",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "편의점 ERP"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "capstone.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "_version": "1.1.0",
    "resource": {
      "js": [
        {
          "uri": "<url>"
        }
      ],
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "capstone.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "Launchpad",
          "target": "Launchpad"
        },

        {
          "pattern": "Stock_Order",
          "name": "Stock_Order",
          "target": "Stock_Order"
        },

        {
          "pattern": "Stock_Order_History",
          "name": "Stock_Order_History",
          "target": "Stock_Order_History"
        },
        
        {
          "pattern": "Stock",
          "name": "Stock",
          "target": "Stock"
        },

        {
          "pattern": "ExpenditureDetails",
          "name": "ExpenditureDetails",
          "target": "ExpenditureDetails"
        },

        {
          "pattern": "ExpirationDate",
          "name": "ExpirationDate",
          "target": "ExpirationDate"
        },

        {
          "pattern": "Promotion",
          "name": "Promotion",
          "target": "Promotion"        
        },

        {
          "pattern": "SalesDetail",
          "name": "SalesDetail",
          "target": "SalesDetail" 
        },

        {
          "pattern": "SalesProfit",
          "name": "SalesProfit",
          "target": "SalesProfit" 
        },

        {
          "pattern": "Schedule",
          "name": "Schedule",
          "target": "Schedule" 
        }
        
      ],

      "targets": {
        "Launchpad": {
          "viewName": "Launchpad"
        },
        
        "Stock_Order": {
          "viewName": "Stock_Order"
        },
        
        "Stock_Order_History": {
          "viewName": "Stock_Order_History"
        },
        
        "Stock": {
          "viewName": "Stock"
        },

        "ExpenditureDetails": {
          "viewName": "ExpenditureDetails"
        },
        

        "ExpirationDate": {
          "viewName": "ExpirationDate"
        },

        "Promotion": {
          "viewName": "Promotion"
        },

        "SalesDetail": {
          "viewName": "SalesDetail"
        },

        "SalesProfit": {
          "viewName": "SalesProfit"
        },

        "Schedule": {
          "viewName": "Schedule"
        }
      }
    }
  
  },
  "components": {
        "capstone.nav": {
            "name": "capstone.nav",
            "settings": {
                "id": "navComponent"
            }
        }
    }
}