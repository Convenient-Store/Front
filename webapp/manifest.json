{
  "sap.app": {
    "id": "capstone",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "편의점 ERP"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "capstone.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "_version": "1.1.0",
    "resource": {
      "js": [
        {
          "uri": "<url>"
        }
      ],
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "capstone.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "Launchpad",
          "target": "Launchpad"
        },
        {
          "pattern": "Stock_Order",
          "name": "Stock_Order",
          "target": "Stock_Order"
      },
      {
        "pattern": "Stock_Order_History",
        "name": "Stock_Order_History",
        "target": "Stock_Order_History"
    },
    {
      "pattern": "Stock",
      "name": "Stock",
      "target": "Stock"
  }

      ],
      "targets": {
        "Launchpad": {
          "viewName": "Launchpad"
        },
        "Stock_Order": {
                    "viewName": "Stock_Order"
                },
        "Stock_Order_History": {
                    "viewName": "Stock_Order_History"
                },
        "Stock": {
                    "viewName": "Stock"
                }
      }
    }
  
  },
  "components": {
        "capstone.nav": {
            "name": "capstone.nav",
            "settings": {
                "id": "navComponent"
            }
        }
    }
}