{
  "sap.app": {
    "id": "capstone",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "편의점 ERP"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "capstone.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "capstone.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "Launchpad",
          "target": "Launchpad"
        }
      ],
      "targets": {
        "Launchpad": {
          "viewName": "Launchpad"
        }
      }
    }
  }
}