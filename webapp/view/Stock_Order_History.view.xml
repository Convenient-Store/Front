<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="capstone.controller.Stock_Order_History"
    xmlns:core="sap.ui.core">
    <Page>
    <headerContent>
            <Bar>
                <contentLeft>
                    <core:Icon src="sap-icon://home" size="25px" press="onHomeIconPress"/>
                </contentLeft>
                <contentRight>
                    <core:Icon src="sap-icon://search" size="25px" class="sapUiIconMargin" press="onSearchIconPress"/>
                    <core:Icon src="sap-icon://bell" size="25px" class="sapUiIconMargin" press="onBellIconPress"/>
                    <core:Icon src="sap-icon://account" size="25px" class="sapUiIconMargin" press="onAccountIconPress"/>
                </contentRight>
            </Bar>
        </headerContent>
        <content>
            <Text text="hello world"/>
        </content>
    </Page>
</mvc:View>